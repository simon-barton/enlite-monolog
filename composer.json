{
    "name": "simon-barton/enlite-monolog",
    "description": "Monolog integration to Zend Framework 2",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeny Shpilevsky",
            "email": "evgeny@shpilevsky.com"
        }
    ],
    "keywords": [
        "Zend Framework 2",
        "Monolog"
    ],
    "require": {
        "monolog/monolog": "~1.6",
        "zendframework/zend-modulemanager": "2.*",
        "zendframework/zend-stdlib": "2.*",
        "zendframework/zend-servicemanager": "2.*",
        "zendframework/zend-log": "2.*",
        "zendframework/zend-code": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": {
            "EnliteMonolog": "src"
        }
    }
}
